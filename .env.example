# ==========================================
# Server-side Environment Variables
# ==========================================
# TS3 Server Query 配置
TS3_SERVER_HOST=1.1.1.1
TS3_QUERY_PORT=10011
TS3_QUERY_USERNAME=serveradmin
TS3_QUERY_PASSWORD=your_query_password_here
TS3_SERVER_ID=1
TS3_VIRTUAL_PORT=9987

# 连接超时时间 (毫秒)
TS3_CONNECTION_TIMEOUT=30000
# 缓存时间
TS3_CACHE_TTL=30000

# Rate Limiting 配置
RATE_LIMIT_WINDOW_MS=60000
# 1/分钟 最多请求次数
RATE_LIMIT_MAX_REQUESTS=30

# ==========================================
# Client-side Environment Variables (公开)
# ==========================================
# 服务器显示配置
NEXT_PUBLIC_SERVER_NAME=米奇妙妙屋
NEXT_PUBLIC_SERVER_DESCRIPTION=欢迎来到我们的游戏语音社区。
NEXT_PUBLIC_SERVER_ADDRESS=ts.example.com
